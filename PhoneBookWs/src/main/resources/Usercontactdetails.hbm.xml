<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class name="com.stpl.gtn.gtn2o.ws.phonebook.UserContactDetails" schema="Awinas"
		table="USERCONTACTDETAILS">

		<id name="cid" type="int">
			<column name="C_ID" not-null="true" unique="true" />
			<generator class="native"></generator>
		</id>
		
		<property name="userid" type="int">
			<column length="30" name="USER_ID" not-null="true" />
		</property>
		<property name="cname" type="string">
			<column length="30" name="C_NAME" not-null="true" />
		</property>
		<property name="cphno" type="string">
			<column length="30" name="C_PHNO" not-null="true" />
		</property>
		<property name="mail" type="string">
			<column length="30" name="C_MAIL" not-null="true" />
		</property>
	</class>
</hibernate-mapping>
